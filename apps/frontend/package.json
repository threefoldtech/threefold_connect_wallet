        "@jimber/stellar-crypto": "^2.0.4",
        "@polkadot/api": "^6.8.1",
        "@polkadot/extension-dapp": "^0.41.2",
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/forms": "^0.4.0",
        "@tailwindcss/line-clamp": "0.3.1",
        "@tailwindcss/typography": "^0.5.0",
        "@vueuse/core": "^7.5.3",
        "@vueuse/integrations": "^7.5.3",
        "axios": "^0.24.0",
