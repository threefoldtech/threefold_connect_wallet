{
    "name": "wallet-frontend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "prebuild": "find ../../node_modules/@polkadot -type f -name 'packageInfo.js' | xargs sed -i \"s/URL('.', import.meta.url).pathname,/URL('.').pathname,/\"",
        "postbuild": "find ../../node_modules/@polkadot -type f -name 'packageInfo.js' | xargs sed -i \"s/URL('.').pathname,/URL('.', import.meta.url).pathname,/\"",
        "upgrade-project": "ncu",
        "tests": "jest"
    },
    "dependencies": {
        "@headlessui/vue": "1.4.3",
        "@heroicons/vue": "^1.0.5",
        "@jimber/pkid": "^1.0.4",
