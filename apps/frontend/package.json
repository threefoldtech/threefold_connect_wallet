    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "prebuild": "find ../../node_modules/@polkadot -type f -name 'packageInfo.js' | xargs sed -i \"s/URL('.', import.meta.url).pathname,/String('path'),/\"",
